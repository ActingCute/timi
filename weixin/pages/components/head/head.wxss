.nav{
    position: fixed;
    width: 100%;
    z-index: 99999;
    border-bottom: 1rpx solid #F5F5F5;
}


.navbar{
    position: relative;
}

.nav-title{    
    position: absolute;    
    transform: translate(-50%, -50%);    
    left: 50%;    
    top: 50%;    
    font-size: 0;    
    font-weight: bold;
    /* 超出隐藏 */
    max-width: 80%;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}

.nav-back{
    position: absolute;    
    transform: translate(-50%, -50%);    
    left: 25rpx;    
    top: 50%;    
    font-size: 0;    
    font-weight: bold;
}
