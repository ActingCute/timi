.nav{
    position: fixed;
    width: 100%;
    z-index: 99999;
    border-bottom: 1rpx solid #F5F5F5;
}


.navbar{
    position: relative;
}

.back-icon{
    height: 100%;
    position: absolute;    
    transform: translateY(-50%);    
}

.back-icon{    
    left: 20rpx;
}

.nav-title{    
    position: absolute;    
    transform: translate(-50%, -50%);    
    left: 50%;    
    top: 50%;    
    font-size: 0;    
    font-weight: bold;
}
